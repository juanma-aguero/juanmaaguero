<!DOCTYPE HTML>
<html>
<head>
    {{ sonata_seo_title() }}
    {{ sonata_seo_metadatas() }}

    {% set theme_base = 'bundles/app/juanmaaguero' %}
    {% block stylesheets %}

        <!-- common css -->
        <link rel="stylesheet" href="{{ asset(theme_base ~ '/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset(theme_base ~ '/css/font-awesome.min.css') }}">
        <link rel="stylesheet" href="{{ asset(theme_base ~ '/css/animate.min.css') }}">
        <link rel="stylesheet" href="{{ asset(theme_base ~ '/css/owl.carousel.css') }}">
        <link rel="stylesheet" href="{{ asset(theme_base ~ '/css/owl.theme.css') }}">
        <link rel="stylesheet" href="{{ asset(theme_base ~ '/css/owl.transitions.css') }}">
        <link rel="stylesheet" href="{{ asset(theme_base ~ '/css/style.css') }}">
        <link rel="stylesheet" href="{{ asset(theme_base ~ '/css/responsive.css') }}">

        <!-- HTML5 shim and Respond.js IE9 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
        <script src="assets/js/html5shiv.js"></script>
        <script src="assets/js/respond.js"></script>
        <![endif]-->


    {% endblock %}
</head>
<body>
{% block wrapper %}{% endblock %}

{% block javascripts %}
    <script type="text/javascript" src="{{ asset(theme_base ~ '/js/jquery-1.11.3.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset(theme_base ~ '/js/bootstrap.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset(theme_base ~ '/js/owl.carousel.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset(theme_base ~ '/js/jquery.stickit.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset(theme_base ~ '/js/menu.js') }}"></script>
    <script type="text/javascript" src="{{ asset(theme_base ~ '/js/scripts.js') }}"></script>
{% endblock %}

</body>
</html>
